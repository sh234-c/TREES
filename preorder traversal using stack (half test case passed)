public:
    vector<int> preorderTraversal(TreeNode*root){
        vector<int>A;
        stack<TreeNode*>S;
        if(root)
        S.push(root);
        while(!S.empty()){
            TreeNode*temp=S.top();
            A.push_back(temp->val);
            S.pop();
            //  if(temp->right){
                
            //     S.push(temp->right);
            //     temp->right=NULL;

            // }
            if(temp->left){
                S.push(temp->left);
                temp->left=NULL;
            }
            
           if(temp->right){
                
                S.push(temp->right);
                temp->right=NULL;

            }
        }
        
        return A;
        
    
    }
        };
